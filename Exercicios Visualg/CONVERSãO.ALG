algoritmo "conversão"
var
R , D: real
C , Q: inteiro
inicio
C <- 1
     Escreval("Quantas vezes vc quer converter?")
     Leia(Q)
     Enquanto (C <= ) faca
             Escreval("Qual o valor em R$? ")
             Leia(R)
             D <- R/5.50
             Escreval("O Valor convertido dá U$:", D:5:2 , " dolares")
             C <- C + 1
     FimEnquanto
fimalgoritmo